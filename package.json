{
  "name": "yokozuna-names",
  "version": "0.0.0-development",
  "description": "Get random Yokozuna names",
  "main": "src/index",
  "scripts": {
    "commit": "git-cz",
    "test": "mocha src/index.test.js -w",
    "test:single": "mocha src/index.test.js",
    "publish": "npm publish",
    "travis-deploy-once": "travis-deploy-once"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mts/yokozuna-names.git"
  },
  "keywords": [
    "random",
    "yokozuna"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mts/yokozuna-names/issues"
  },
  "homepage": "https://github.com/mts/yokozuna-names#readme",
  "dependencies": {
    "unique-random-array": "1.0.1"
  },
  "devDependencies": {
    "chai": "4.2.0",
    "commitizen": "3.0.2",
    "cz-conventional-changelog": "2.1.0",
    "ghooks": "2.0.4",
    "mocha": "5.2.0",
    "semantic-release": "^15.9.17",
    "travis-deploy-once": "^5.0.9"
  },
  "czConfig": {
    "path": "node_modules/cz-conventional-changelog"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run test:single"
    }
  }
}
