{
  "name": "yokozuna-names",
  "version": "1.5.1",
  "description": "Get random Yokozuna names",
  "main": "dist/index",
  "scripts": {
    "commit": "git-cz",
    "test": "mocha src/index.test.js --compilers js:babel-register",
    "test:watch": "npm t -- -w",
    "cover": "nyc npm t",
    "publish": "semantic-release && npm publish",
    "travis-deploy-once": "travis-deploy-once",
    "semantic-release": "semantic-release",
    "check-coverage": "nyc check-coverage --statements 100 --branches 100 --functions 100 --lines 100",
    "report-coverage": "cat ./coverage/lcov.info | codecov",
    "prebuild": "rimraf dist",
    "build": "babel --copy-files --out-dir dist --ignore *.test.js src"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mts/yokozuna-names.git"
  },
  "keywords": [
    "random",
    "yokozuna"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mts/yokozuna-names/issues"
  },
  "homepage": "https://github.com/mts/yokozuna-names#readme",
  "dependencies": {
    "unique-random-array": "1.0.1"
  },
  "devDependencies": {
    "@phoenix/buttons": "0.0.2",
    "@phoenix/primer": "0.0.2",
    "babel-cli": "6.26.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "babel-register": "6.26.0",
    "chai": "4.2.0",
    "codecov.io": "0.1.6",
    "commitizen": "3.0.2",
    "cz-conventional-changelog": "2.1.0",
    "ghooks": "2.0.4",
    "mocha": "5.2.0",
    "nyc": "13.0.1",
    "rimraf": "2.6.2",
    "semantic-release": "^15.9.17",
    "travis-deploy-once": "^5.0.9"
  },
  "czConfig": {
    "path": "node_modules/cz-conventional-changelog"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run cover && npm run check-coverage"
    }
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ]
  },
  "files": [
    "dist",
    "README.md"
  ]
}
